# See https://editorconfig.org/#file-format-details
root = true

[*]
charset = utf-8
end_of_line = lf
insert_final_newline = true
trim_trailing_whitespace = true

[*.bt]
indent_style = tab
indent_size = 4

[*.yml]
indent_style = space
indent_size = 2

# [*.md]
# Whitespace will also be trimmed in markdown files. Use <br> for linebreaks.
