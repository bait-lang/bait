fun more_generic_types[G, H](g G, h H) string {
	return typeof(g) + '_' + typeof(h)
}

fun test_multiple_types(){
	assert more_generic_types(1, 2) == 'i32_i32'
	assert more_generic_types('str', false) == 'string_bool'
}
