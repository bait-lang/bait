struct Container{
	// Note: Foo must be used before definition
	arr []Foo
}

struct Foo{}

fun (arr []Foo) perform() i32 {
	return arr.length
}

fun test_method_on_array_of_user_type() {
	// This code just has to compile

	f := [Foo{}, Foo{}]
	assert f.perform() == 2

	c := Container{}
	assert c.arr.perform() == 0
}
