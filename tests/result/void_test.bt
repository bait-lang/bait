fun void_error() ! {
	return error('void')
}

fun void_return() ! {
	return
}

fun test_void_error() {
	mut errs := 0
	void_error() or {
		assert err == 'void'
		errs += 1
	}
	assert errs == 1
}

fun test_void_return() {
	void_return() or {
		assert false
	}
}
