fun test_bitwise_operators() {
	a := 0b1001
	b := 0b0011

	assert ~a == -10
	assert a & b == 0b0001
	assert a | b == 0b1011
	assert a ^ b == 0b1010

	assert a << 2 == 0b100100
	assert a >> 2 == 0b10
}
