fun call() ![]i32 {
	return error("")
}

fun test_tmpvar_conflict() {
	// `or` blocks are generated as if-else with temporary variables. The tmp var for `_` should not conflict
	r := call() or {
		_ = err
		[]i32
	}
	assert r.length == 0
}
