import bait.lexer
import bait.token

fun test_unclosed_string() {
	text := '"no
closing
quote'

	mut l := lexer.Lexer{}
	l.init(text, .discard)

	assert l.next() == .error
	assert l.val == 'unclosed string literal'
	assert l.get_pos() == token.Pos{
		line = 1
		col = 2
	}
}
