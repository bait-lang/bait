import bait.parser

fun test_unknown_comptime_var() {
	text := '_ := $CT_VAR_XXX'

	mut p := parser.new_default()
	p.init(text, '/memory/text.bt', 'main')

	file := p.parse()
	assert file.errors.length == 1
	assert file.errors[0].msg == 'unsupported comptime var `$CT_VAR_XXX`'
}
