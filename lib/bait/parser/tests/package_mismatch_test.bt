import bait.ast
import bait.parser
import bait.preference

fun test_valid_numbers() {
	text := 'package different'

	mut p := parser.new(ast.new_table(), preference.Prefs{})
	p.init(text, '/just/for/test/file.bt', 'match')

	file := p.parse()
	assert file.path.length == 0
	assert file.infos.length == 1
	assert file.infos[0].msg.contains('package mismatch')
}
