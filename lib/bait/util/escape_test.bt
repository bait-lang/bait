import util

fun test_escape_char(){
	assert util.escape_char('"', `\"`) == '\"'
	assert util.escape_char('\"', `\"`) == '\"'
	assert util.escape_char('\\"', `\"`) == '\\\"'
	assert util.escape_char('\\\"', `\"`) == '\\\"'
}
