package ast

import bait.token

pub struct Attribute{
	name string
	value string
	pos token.Pos
}

pub fun (attrs []Attribute) find_attr(name string) Attribute {
	for attr in attrs {
		if attr.name == name {
			return attr
		}
	}
	return Attribute{}
}
