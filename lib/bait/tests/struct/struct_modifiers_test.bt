import bait.tests.packages.struct_mod

fun test_struct_modifiers(){
	mut f := struct_mod.Foo{}
	assert f.get_a() == 1
	assert f.c == 1
	assert f.d == 20

	f.add_b(8)
	assert f.get_b() == 10

	f.sub_d(5)
	assert f.d == 15

	assert f.e == 0
	f.e = 5
	f.e -= 3
	assert f.e == 2
}
