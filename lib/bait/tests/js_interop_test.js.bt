import 'fs' as #JS.fs

const #JS.name := #JS.String

fun #JS.fs.existsSync(path #JS.String) #JS.Boolean

fun test_js_interop(){
	jres := #JS.fs.existsSync($FILE.str)
	assert typeof(jres) == 'JS.Boolean'

	bres := jres as bool
	assert bres
}

fun test_typeof() {
	assert typeof(#JS.name) == 'JS.String'
}
