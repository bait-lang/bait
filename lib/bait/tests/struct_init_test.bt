struct Account{
	balance i32 := 50
}

fun test_default_field_value() {
	a := Account{}
	assert a.balance == 50
}

type Int := i32

struct Foo{
	m i32
	n Int
}

fun test_alias_default_value(){
	f := Foo{}
	assert f.m == 0
	assert f.n == 0 as Int
}
