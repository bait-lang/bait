pub fun test_index_and_last_index() {
	nums := [1, 2, 1, 2]
	assert nums.index(1) == 0
	assert nums.last_index(1) == 2

	strings := ['a', 'b', 'c', 'c']
	assert strings.index('c') == 2
	assert strings.last_index('c') == 3
}

