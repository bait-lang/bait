pub fun test_reverse() {
	a := [1, 2, 3]
	b := a.reverse()
	assert a == [1, 2, 3]
	assert b == [3, 2, 1]
}

pub fun test_reverse_in_place() {
	a := [1, 2, 3]
	a.reverse_in_place()
	assert a == [3, 2, 1]
}
