package builtin

pub fun println(s string) {
	#C.printf('%s\n'.str, s.str)
}

pub fun eprintln(s string) {
	#C.fprintf(#C.stderr, '%s\n'.str, s.str)
}

pub fun exit(code i32) {
	#C.exit(code)
}
