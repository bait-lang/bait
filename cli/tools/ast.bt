package main

import os
import bait.ast
import bait.tokenizer
import bait.parser

fun main() {
	path := os.ARGS[2]
	text := os.read_file(path)
	tokens := tokenizer.tokenize(text, path)
	ast := parser.parse(tokens, path, ast.new_table())
	println(ast)
}
