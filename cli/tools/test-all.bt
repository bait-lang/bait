package main

import os

const TOOLS := ['build-examples', 'build-tools']

fun main() {
	mut fails := 0
	for tool in TOOLS {
		cmd := 'node ${$BAITEXE} ${tool}'
		code := os.system(cmd)
		if code != 0 {
			fails += 1
		}
	}
	md_code := os.system('node ${$BAITEXE} check-md docs/docs.md')
	if md_code != 0 {
		fails += 1
	}
	if fails > 0 {
		exit(1)
	}
}
