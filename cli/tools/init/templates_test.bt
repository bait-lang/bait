import os
import bait.util.testing

fun test_templates_compile() {
	mut runner := testing.BuildRunner{
		root = os.dir($FILE) + '/templates'
		only_dirs = true
		dir_suffix = '/src'
	}
	runner.build_all_in_root()
	assert runner.oks == 1
}
